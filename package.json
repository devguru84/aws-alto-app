{
  "name": "react-typescript-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/*.{ts,tsx}' --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit"
